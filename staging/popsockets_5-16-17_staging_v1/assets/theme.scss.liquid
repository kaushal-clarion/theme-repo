.masonry-feature {
  position: relative;
  display: inline-block;
  vertical-align: top;
  overflow: hidden;
  height: 0;

  {% if settings.masonry-features-gutters %}
    margin: 0 0 10px 0;
    padding: 0 5px;
  {% endif %}

  @include breakpoint(s) { text-align: center; }

  &.has-link { cursor: pointer; }

  figure {
    position: relative;
    height: 0;
    overflow: hidden;

    @include breakpoint(s) {
      display: inline-block;
      height: auto;

      &:not(.no-image) {
        padding-bottom: 0 !important;
      }
    }

    .has-1-features & {
      height: auto;
    }
  }

  figure:after {
    content: '';
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    @include transition(all 0.2s $ease-out-quad);
  }

  .has-animation &:hover {
    img { @include transform(scale(1.02)); }

    &.color-black figure:after { background: rgba(255,255,255,0.5); }
    &.color-white figure:after { background: rgba(0,0,0,0.5); }
  }

  img {
    width: auto;
    max-width: none;
    -webkit-backface-visibility: hidden;
    @include transform-style(preserve-3d);
    @include transition(transform 0.2s $ease-out-quad);

    @include breakpoint(s) {
      max-width: 100%;
      width: 100% !important;
      height: auto !important;
      margin: 0 !important;
    }
  }

  .no-image {
    width: 100%;
    background: mix($border-color, $body-background-color, 30%);

    @include breakpoint(s) {
      padding-bottom: 30% !important;
    }
    .masonry-hover{
      img{
          margin-top: 0px !important;
      }
    }
  }
}

.masonry-feature-text {
  position: absolute;
  top: 50%;
  left: 50%;
  max-width: 85%;
  width: 100%;
  text-align: center;
  font-size: $base-font-size;
  -webkit-font-smoothing: antialiased;

  p { margin: 0; }
}

{% if settings.masonry-features-hover-text %}
  .no-touch .masonry-feature-text {
    @include opacity(0);
    @include transform(scale(0.95));
    @include transition(all 0.2s $ease-out-quad);
  }
  .no-touch .alternative-masonry-feature:hover .masonry-feature-text,
  .no-touch .masonry-feature:hover .masonry-feature-text {
    @include transform(scale(1));
    @include opacity(1);
  }
{% endif %}


