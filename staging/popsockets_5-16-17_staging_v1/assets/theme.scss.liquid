  margin: 0 auto;
}

.search-results-item {
  border-bottom: 1px solid $border-color;
  padding-bottom: 20px;
  margin-bottom: 20px;

  &:last-child {
    border-bottom: 0;
    padding-bottom: 0;
    margin-bottom: 0;
  }
}

.search-result-title {
  text-transform: none;
  letter-spacing: 0;
  margin-bottom: 5px;
  font-size: 14px;
}

.search-result-link {
  color: $heading-color;

  &:hover {
