.navigation.mobile {
  @media (max-width:1037px){
  position: absolute;
  top: 0;
  bottom: 0;
  background: $body-background-color;
  z-index: 3000;
  max-width:300px;
  width: 100%;
  overflow: hidden;
  @include transform(translateX(-300px));
  @include transition(all 0.2s $ease-out-quad);
  @include breakpoint(xs) {
    @include transform(translateX(-300px));
    max-width: 300px;
  }
  &.visible {
   
      @include transform(translateX(0px));
    
  }
  .branding {
    position: relative;
    font-size: 0;
    img { height: auto; }
  }
  .mobile-nav-logo {
    display: inline-block;
    max-width: 200px;
  }
  .sticky-logo { display: none; }
  .logo-regular {
    display: block;
  }
  .has-retina {
    @include retina {
      .logo-regular {
        display: none;
      }
      .logo-retina {
        display: block;
      }
    }
  }
  ul {
    position: relative;
    padding: 0 25px;
    @include transition(all 0.2s $ease-out-quad);
    &.out-of-view {
      @include transform(translateX(-100%));
    }
  }
  ul ul {
    position: absolute;
    top: 0;
    left: 100%;
    width: 100%;
    &.in-view {
      display: block;
    }
  }
  .enter-linklist {
    @extend %icon;
    display: inline-block;
    float: right;
    margin-top: -7px;
    border: 1px solid $border-color;
    border-radius: 2px;
    padding: 7px 12px;
    color: $heading-color;
  }
  .back {
    position: relative;
    display: none;
    padding: 15px 20px;
    border-bottom: 1px solid $border-color;
    cursor: pointer;
    font-family: $navigation-font;
    font-weight: $navigation-weight;
    {% if settings.navigation-font-small-caps %}
      @extend %small-caps;
    {% endif %}
    @include breakpoint(m) {
      display: block;
    }
    .icon {
      @extend %icon;
      position: absolute;
      top: 50%;
      left: 0;
      padding: 0;
      border: 0;
      @include transform(translateY(-50%));
    }
  }
  .first > a {
    border-top: 1px solid $border-color;
  }
  a {
    display: block;
    padding: 12px 5px;
    border-bottom: 1px solid $border-color;
  }
  .cart-count {
    display: none;
  }
  .mobile-link {
    display: block;
  }
  .navigation-toggle { padding-top: 0; }
  .search-form {
    position: relative;
    padding: 20px;
    @media(max-width:1037px){
    	padding: 20px 0px 20px 5px;
    }
    margin: 0;
    input[type="submit"] {
      right: 20px;
      @media(max-width:1037px){
    	right: 0px;
    }
    }
  }
  .search-input {
    max-width: 100%;
    width: 100%;
  }
}
}



.navigation.full-width {
  @media (min-width:1038px){
  display: table;
  width: 100%;
  text-align: center;
  position: relative;
  font-size: 0;
  -webkit-font-smoothing: antialiased;
  padding-top:0px;
  {% if settings.header-border %}
    border-bottom: 1px solid $border-color;
  {% endif %}
  @include breakpoint(m) {
    display: none;
  }
  .branding,
  > ul,
  .cart-count {
    display: table-cell;
    vertical-align: middle;
  }
  .branding {
    width: 145px;
    border-bottom: 0;
    margin: 0;
    padding: 7px 0 10px 30px;
    text-align: left;
    @include opacity(0);
    .logo-regular {
      display: inline-block;
      vertical-align: middle;
    }
    img {
      display: block;
      max-height: 37px;
      -webkit-backface-visibility: hidden;
      backface-visibility: hidden;
      .ie8 & {
        filter: alpha(opacity=0);
        max-height: 42px;
        padding-top: 2px;
      }
    }
  }
  .cart-count {
    width: 100px;
    padding: 0 30px 0 0;
    border: 0;
    line-height: 1;
    font-size: 13px;
    font-family: $heading-font;
    color: $header-tools-color;
    text-align: right;
    @include opacity(0);
    @include transition(all 0.2s $ease-out-quad);
    {% if settings.heading-font-small-caps %}
      @extend %small-caps;
      font-size: 12px;
    {% endif %}
    span {
      line-height: 1.6;
      vertical-align: middle;
    }
  }
  li {
    display: inline-block;
    margin-left: 0px;
  }
  .enter-linklist {
    display: none;
  }
  a {
    position: relative;
    top: 1px;
    display: block;
    padding: 15px 25px 20px;
    padding-top: 20px;
    font-size: 20px;
    border: 1px solid transparent;
    z-index: 2000;
    @include transition(line-height 0.2s $ease-out-quad);
    {% if settings.navigation-font-small-caps %}
      font-size: 14px;
      padding: 17px 25px;
    {% endif %}
  }
  .has-dropdown,
  .has-mega-nav {
    > a {
      padding-right: 25px;
      padding-left:20px;
      &:after {
        @extend %icon;
        content:"\e602";
        font-size: 6px;
        position: absolute;
        top: 45%;
        right: 8px;
         font-family:'chiko-icons';
        {% if settings.navigation-font-small-caps %}
          margin-top: -1px;
        {% endif %}
      }
    }
    &:hover > a {
      border-bottom: 1px solid $body-background-color;
      background: $body-background-color;
      color: $heading-color;
    }
  }
  .has-dropdown {
    position: relative;
    &:hover > ul {
      display: block;
    }
    > ul {
      position: absolute;
      top: 51px;
      @media(max-width: 1060px){
       top: 49px;
      }
      z-index: 1000;
      background: $body-background-color;
      border: 1px solid $border-color;
      text-align: left;
      padding: 15px 0;
      
      li {
        display: inline-block;
        width: 145px;
      }
      a {
        display: block;
        border: 0;
        padding: 1px 25px;
        line-height: 1.6em;
        font-family: $body-font;
        font-weight: 400;
        text-transform: none;
        letter-spacing: 0;
        font-size: $base-font-size;
        border: 0;
        &:hover {
          color: mix($navigation-color, $body-background-color, 50%);
        }
      }
    }
  }
  .mega-nav-list {
    display: inline-block;
    vertical-align: top;
    width: 33.333%;
    padding-right: 30px;
    li {
      display: block;
    }
    a {
      padding: 1px 0;
      line-height: 1.6;
      font-family: $body-font;
      font-weight: 400;
      text-transform: none;
      letter-spacing: 0;
      font-size: $base-font-size;
      border: 0;
      &:hover {
        color: $body-color;
      }
    }
  }
  .mobile-link {
    display: none;
  }
}
}


