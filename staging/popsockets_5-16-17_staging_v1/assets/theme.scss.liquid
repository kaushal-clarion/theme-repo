.product-description {
  margin-top: 40px;
  text-align: left;
}


.template-search .main-content {
  margin-bottom: 65px;
}

.no-search {
  text-align: center;
  padding: 0 20px;
}

.search-results-wrapper {
  margin-top: 40px;
