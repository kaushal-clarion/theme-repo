    font-size: 16px;
  {% endif %}
}

.product-title {
  @extend %heading-text;
  margin: 5px 0;
  font-size: 35px;

  {% if settings.heading-font-small-caps %}
    font-size: 32px;
  {% endif %}
}

.product-price {
  margin: 0 0 40px;
  font-size: 18px;
}

.product-price-compare {
  margin-left: 8px;
