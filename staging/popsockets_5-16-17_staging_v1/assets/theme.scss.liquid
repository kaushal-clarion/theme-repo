$i: 1;
@while $i < 6 {
  .slide-#{$i} {
    h3,
    p {
      color: nth($slide-colors, $i);
    }
  }
  $i: $i + 1;
}

.slide-content {
  position: absolute;
  top: 50%;
  left: 70px;
  right: 70px;
  @include transform(translateY(-50%));

  @include breakpoint(s) {
    left: 30px;
    right: 30px;
  }

  .content-aligned-center & {
    text-align: center;
  }

  .content-aligned-right & {
    text-align: right;
  }
}

.slide-content-inner {
  margin: 0 auto;
  max-width: $max-width;
}

.slide-title {
  @extend %heading-text;
  margin: 0;
  font-size: 64px;

  {% if settings.heading-font-small-caps %}
    font-size: 60px;
  {% endif %}

  @include breakpoint(m) {
    font-size: 50px;

    {% if settings.heading-font-small-caps %}
      font-size: 45px;
    {% endif %}
  }

  @include breakpoint(s) {
    font-size: 32px;

    {% if settings.heading-font-small-caps %}
      font-size: 28px;
    {% endif %}
  }

  @include breakpoint(xs) {
    font-size: 18px;

    {% if settings.heading-font-small-caps %}
      font-size: 26px;
    {% endif %}
  }
}

.slide-subtitle {
  margin: 5px 0 0;
  font-size: 21px;

  @include breakpoint(s) { font-size: 16px; }
  @include breakpoint(xs) { font-size: 14px; }
}

