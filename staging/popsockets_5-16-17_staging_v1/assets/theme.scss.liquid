/* Masonry features */

%text-color {
  .color-white &,
  .color-white & a {
    color: #fff;
    text-shadow: 0px 0px 10px rgba(0, 0, 0, 0.3);
  }

  .color-black &,
  .color-black & a {
    color: #000;
    text-shadow: 0px 0px 10px rgba(255, 255, 255, 0.3);
  }
}

%left {
  {% unless settings.masonry-features-layout == "content-width" %}
    padding-left: 0;
  {% endunless %}
}

%right {
  {% unless settings.masonry-features-layout == "content-width" %}
    padding-right: 0;
  {% endunless %}
}

%bottom {
  {% unless settings.masonry-features-layout == "content-width" %}
    margin-bottom: 0;

    @include breakpoint(s) {
      margin-bottom: 10px;
    }
  {% endunless %}

  {% unless settings.masonry-features-gutters %}
    @include breakpoint(s) {
      margin-bottom: 0;
    }
  {% endunless %}
}


